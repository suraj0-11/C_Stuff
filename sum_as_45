#include<stdio.h>
void main()
{
    int a[50],n,i,j,s;
    printf("Enter the size of the array");
    scayrnf("%d",&n);
    printf("Enter the elements of the array");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        for(j=i+1;j<n;j++)
        {
            s = a[i] + a[j];
            if(s == 45) 
            {
                printf("The elements are %d and %d "
            }
        }
    }#include<stdio.h>
